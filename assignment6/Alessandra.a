        .globl  main
        .data
K:      .word   20
Y:      .word   56
Z:      .word   0:50    # array of 50 $zeros

        .text
main:
        lw      $t1, K
        lw      $t0, Y
        sra     $t1, $t1, 2     #K/4
        addi    $t1, $t1, 210   #K/4 + 210
        sll     $t1, $t1, 4     # _ x 16
        sub     $t2, $t0, $t1   #t2 = Y - 16 * (K / 4 +210)
        lw      $t1, K
        sll     $t1, $t1, 2
        la      $t0, Z
        addiu   $t0, $t0, $t1
        sw      $t2, ($t0)      #z[20] = t2
        jr      $ra
